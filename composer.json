{
    "name": "boxybird/rosebud",
    "description": "A TMDB (The Movie Database) API wrapper. Includes Data Transfer Objects (DTO).",
    "license": "MIT",
    "keywords": [
        "php",
        "dto",
        "data transfer objects",
        "tmdb",
        "wrapper",
        "movie",
        "cinema",
        "tv",
        "tv show",
        "tv episode"
    ],
    "authors": [
        {
            "name": "Andrew Rhyand",
            "email": "hello@andrewrhyand.com"
        }
    ],
    "require-dev": {
        "illuminate/collections": "^11.34",
        "illuminate/http": "^11.34",
        "nesbot/carbon": "^3.8",
        "pestphp/pest": "^3.7",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Rosebud\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
